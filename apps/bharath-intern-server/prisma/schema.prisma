datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  age         Int?
  email       String? @unique
  firstName   String?
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  lastName    String?
  password    String
  phoneNumber String?
  username    String  @unique
}
